<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN" "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">
<fontscolors>
    <fontcolor foreColor="ff99006b" name="TODO">
        <font style="bold"/>
    </fontcolor>
    <fontcolor default="keyword" name="ANY_KEYWORD"/>
    <fontcolor foreColor="ff99006b" name="COMMENT_TODO">
        <font style="bold"/>
    </fontcolor>
    <fontcolor name="mod-constructor">
        <font style="bold"/>
    </fontcolor>
    <fontcolor default="error" name="ERROR" waveUnderlined="red"/>
    <fontcolor name="mod-enum"/>
    <fontcolor default="string" name="STRING_TEXT"/>
    <fontcolor foreColor="ffeddd37" name="STRING_ESCAPE">
        <font style="bold"/>
    </fontcolor>
    <fontcolor foreColor="ffd1ceff" name="GLOBAL_VAR">
        <font style="italic"/>
    </fontcolor>
    <fontcolor name="mod-deprecated" strikeThrough="darkGray"/>
    <fontcolor default="default" foreColor="blue" name="COMMENT_LINK" underline="blue"/>
    <fontcolor foreColor="ff8aa6c1" name="mod-parameter"/>
    <fontcolor foreColor="ffd27522" name="CONSTANT"/>
    <fontcolor foreColor="ff99ffff" name="CLASS_VAR">
        <font style="bold+italic"/>
    </fontcolor>
    <fontcolor default="string" name="STRING_LITERAL"/>
    <fontcolor name="mod-static">
        <font style="italic"/>
    </fontcolor>
    <fontcolor default="field" foreColor="ffd1ceff" name="INSTANCE_VAR"/>
    <fontcolor default="number" foreColor="ffeddd3d" name="INT_LITERAL"/>
    <fontcolor bgColor="ff38566f" name="mark-occurrences"/>
    <fontcolor foreColor="ff2e92c7" name="TYPE_SYMBOL"/>
    <fontcolor default="comment" name="COMMENT_TEXT"/>
    <fontcolor default="number" foreColor="ff66cc00" name="FLOAT_LITERAL"/>
    <fontcolor default="number" foreColor="ff66cc00" name="DOUBLE_LITERAL"/>
    <fontcolor foreColor="white" name="mod-field">
        <font style="bold"/>
    </fontcolor>
    <fontcolor foreColor="fff3c55f" name="mod-method">
        <font style="bold"/>
    </fontcolor>
    <fontcolor default="comment" name="LINE_COMMENT"/>
    <fontcolor name="mod-local-variable"/>
    <fontcolor default="whitespace" name="WHITESPACE"/>
    <fontcolor foreColor="ffca4344" name="REGEXP_LITERAL"/>
    <fontcolor foreColor="ffd27522" name="mod-class"/>
    <fontcolor name="mod-unused" waveUnderlined="ffff8888"/>
    <fontcolor foreColor="ffff006b" name="STRING_INVALID" strikeThrough="ffff006b">
        <font style="bold"/>
    </fontcolor>
    <fontcolor default="operator" name="ANY_OPERATOR"/>
    <fontcolor default="identifier" name="IDENTIFIER"/>
    <fontcolor default="string" name="QUOTED_STRING_LITERAL"/>
    <fontcolor default="number" foreColor="ff66cc00" name="LONG_LITERAL"/>

    <!-- Code Coverage -->
    <fontcolor name="covered" bgColor="ff2b492b"/>
    <fontcolor name="uncovered" bgColor="ff672b2b"/>
    <fontcolor name="inferred" bgColor="ff2b3a2b"/>

</fontscolors>
